<div class="container p-3 mb-3" >

<%= form_with model: task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
    <div>
      <%= f.label :completed %>
      <%= f.check_box :completed %>
    </div>
  <%= f.submit %>
<% end %>
</div>
